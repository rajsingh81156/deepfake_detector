# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration
# For local MongoDB: mongodb://127.0.0.1:27017/verimedia
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/verimedia
MONGO_URI=mongodb://127.0.0.1:27017/verimedia

# JWT Secret (use a strong, random string in production)
JWT_SECRET=your_secure_jwt_secret_here

# AI Service Configuration
AI_SERVICE_URL=http://127.0.0.1:8000/analyze

# Frontend URL for CORS
FRONTEND_URL=http://localhost:5173